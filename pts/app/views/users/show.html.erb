<div class="col-xs-6 col-sm-6 col-md-6 col-lg-6">
  <div class="panel panel-success">
      <div class="panel-heading">
        <h3 class="panel-title">Devam Eden Görevler</h3>
      </div>
      <div class="panel-body">
        <ul class="list-group">
        <% @contunied.each do |contunied| %>
          <li class="list-group-item"><%= link_to contunied.description[0..20], contunied %></li>
        <% end %>
        </ul>
        <%= will_paginate @contunied,:param_name => 'contunied_page', renderer: BootstrapPagination::Rails %>
      </div>
  </div>
</div>
<div class="col-xs-6 col-sm-6 col-md-6 col-lg-6">
  <div class="panel panel-primary">
      <div class="panel-heading">
        <h3 class="panel-title">Bitmiş Görevler</h3>
      </div>
      <div class="panel-body">
        <ul class="list-group">
        <% @finished.each do |finished| %>
          <li class="list-group-item"><%= link_to finished.description[0..20], finished %></li>
        <% end %>
        </ul>
        <%= will_paginate @finished, :param_name => 'finished_page', renderer: BootstrapPagination::Rails %>
      </div>
  </div>
</div>